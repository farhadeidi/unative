{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "module": "ES6",
    "moduleResolution": "bundler",
    "lib": ["esnext", "dom"],
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": false,
    "composite": false,
    "strict": true,
    "esModuleInterop": true
  }
}
