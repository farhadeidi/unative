{
  "name": "unative",
  "version": "0.0.6",
  "description": "One UI Library for Web and Mobile",
  "type": "module",
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "prepare": "tsup"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farhadeidi/unative.git"
  },
  "author": "Farhad Eidi <farcom7@gmail.com> (https://github.com/farhadeidi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farhadeidi/unative/issues"
  },
  "homepage": "https://unative.dev",
  "exports": {
    ".": {
      "browser": {
        "types": "./index.native.d.ts",
        "require": "./index.native.cjs",
        "default": "./index.native.js"
      },
      "react-native": {
        "types": "./index.native.d.ts",
        "require": "./index.native.cjs",
        "default": "./index.native.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    },
    "./ui": {
      "react-native": {
        "types": "./ui/index.native.d.ts",
        "require": "./ui/index.native.cjs",
        "import": "./ui/index.native.js"
      },
      "default": {
        "types": "./ui/index.d.ts",
        "require": "./ui/index.cjs",
        "import": "./ui/index.js"
      }
    },
    "./hooks": {
      "react-native": {
        "types": "./hooks/index.native.d.ts",
        "require": "./hooks/index.native.cjs",
        "import": "./hooks/index.native.js"
      },
      "default": {
        "types": "./hooks/index.d.ts",
        "require": "./hooks/index.cjs",
        "import": "./hooks/index.js"
      }
    },
    "./native": {
      "types": "./native/index.d.ts",
      "require": "./native/index.cjs",
      "import": "./native/index.js"
    },
    "./native/metro": {
      "types": "./native/metro/index.d.ts",
      "require": "./native/metro/index.cjs",
      "import": "./native/metro/index.js"
    },
    "./types": "./types.d.cts"
  },
  "keywords": [],
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "~18.3.12",
    "nativewind": "*",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-safe-area-context": "4.12.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.2",
    "zustand": "^5.0.2"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "*",
    "clsx": "^2.1.1",
    "nativewind": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-safe-area-context": "*",
    "react-native-web": "*",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    },
    "react-native-safe-area-context": {
      "optional": true
    },
    "@react-native-async-storage/async-storage": {
      "optional": true
    }
  },
  "dependencies": {
    "zustand": "^5.0.2"
  }
}
