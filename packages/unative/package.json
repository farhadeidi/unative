{
  "name": "unative",
  "version": "0.0.19",
  "description": "One UI Library for Web and Mobile",
  "type": "module",
  "scripts": {
    "watch-changes": "rollup -c --watch",
    "watch": "pnpm watch-changes",
    "build": "rm -rf dist && rollup -c",
    "prepare": "",
    "lint": "eslint src",
    "generate-package": "npx tsx ./package-json-generator.ts"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farhadeidi/unative.git"
  },
  "author": "Farhad Eidi <farcom7@gmail.com> (https://github.com/farhadeidi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farhadeidi/unative/issues"
  },
  "homepage": "https://unative.dev",
  "keywords": [],
  "dependencies": {
    "@unative/primitives": "workspace:*"
  },
  "devDependencies": {
    "@gorhom/bottom-sheet": "^5.0.6",
    "@react-native-segmented-control/segmented-control": "2.5.4",
    "@react-navigation/native": "^7.0.14",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "~18.3.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild-react18-useclient": "^1.0.7",
    "glob": "^11.0.1",
    "lucide-react": "^0.471.1",
    "lucide-react-native": "^0.474.0",
    "nativewind": "*",
    "next-themes": "^0.4.4",
    "react": "18.3.1",
    "react-native": "0.76.6",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-svg": "15.8.0",
    "remeda": "^2.19.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "@gorhom/bottom-sheet": "*",
    "@react-native-async-storage/async-storage": "*",
    "@react-native-segmented-control/segmented-control": "2.5.4",
    "class-variance-authority": "*",
    "clsx": "*",
    "lucide-react": "*",
    "lucide-react-native": "*",
    "nativewind": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-reanimated": "*",
    "react-native-safe-area-context": "*",
    "react-native-svg": "*",
    "react-native-web": "*",
    "tailwind-merge": "*",
    "tailwindcss": "*",
    "zustand": "*"
  },
  "exports": {
    ".": {
      "react-native": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "react-server": {
        "types": "./web.d.ts",
        "default": "./web.js"
      },
      "require": {
        "types": "./web.d.ts",
        "default": "./web.cjs"
      },
      "import": {
        "types": "./web.d.ts",
        "default": "./web.js"
      },
      "default": {
        "types": "./web.d.ts",
        "default": "./web.js"
      }
    },
    "./provider": {
      "react-native": {
        "types": "./provider/index.d.ts",
        "default": "./provider/index.js"
      },
      "react-server": {
        "types": "./provider/web.d.ts",
        "default": "./provider/web.js"
      },
      "require": {
        "types": "./provider/web.d.ts",
        "default": "./provider/web.cjs"
      },
      "import": {
        "types": "./provider/web.d.ts",
        "default": "./provider/web.js"
      },
      "default": {
        "types": "./provider/web.d.ts",
        "default": "./provider/web.js"
      }
    },
    "./types": {
      "types": "./types/index.d.ts",
      "require": "./types/index.cjs",
      "import": "./types/index.js",
      "default": "./types/index.js"
    },
    "./provider/web": {
      "types": "./provider/web.d.ts",
      "require": "./provider/web.cjs",
      "import": "./provider/web.js",
      "default": "./provider/web.js"
    },
    "./provider/native": {
      "types": "./provider/native.d.ts",
      "require": "./provider/native.cjs",
      "import": "./provider/native.js",
      "default": "./provider/native.js"
    },
    "./with-unative": {
      "types": "./with-unative/index.d.ts",
      "require": "./with-unative/index.cjs",
      "import": "./with-unative/index.js",
      "default": "./with-unative/index.js"
    },
    "./hooks": {
      "types": "./hooks/index.d.ts",
      "require": "./hooks/index.cjs",
      "import": "./hooks/index.js",
      "default": "./hooks/index.js"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.cjs",
      "import": "./lib/*.js",
      "default": "./lib/*.js"
    },
    "./ui": {
      "react-native": {
        "types": "./ui/index.d.ts",
        "default": "./ui/index.js"
      },
      "react-server": {
        "types": "./ui/web.d.ts",
        "default": "./ui/web.js"
      },
      "require": {
        "types": "./ui/web.d.ts",
        "default": "./ui/web.cjs"
      },
      "import": {
        "types": "./ui/web.d.ts",
        "default": "./ui/web.js"
      },
      "default": {
        "types": "./ui/web.d.ts",
        "default": "./ui/web.js"
      }
    },
    "./ui/*": {
      "react-native": {
        "types": "./ui/*/native.d.ts",
        "default": "./ui/*/native.js"
      },
      "react-server": {
        "types": "./ui/*/web.d.ts",
        "default": "./ui/*/web.js"
      },
      "require": {
        "types": "./ui/*/web.d.ts",
        "default": "./ui/*/web.cjs"
      },
      "import": {
        "types": "./ui/*/web.d.ts",
        "default": "./ui/*/web.js"
      },
      "default": {
        "types": "./ui/*/web.d.ts",
        "default": "./ui/*/web.js"
      }
    },
    "./ui/*/native": {
      "types": "./ui/*/native.d.ts",
      "require": "./ui/*/native.cjs",
      "import": "./ui/*/native.js",
      "default": "./ui/*/native.js"
    },
    "./ui/*/web": {
      "types": "./ui/*/web.d.ts",
      "require": "./ui/*/web.cjs",
      "import": "./ui/*/web.js",
      "default": "./ui/*/web.js"
    }
  }
}
