{
  "compilerOptions": {
    "jsx": "preserve",
    "declaration": false,
    "declarationMap": false,
    "module": "Preserve",
    "moduleResolution": "bundler",
    "types": ["nativewind/types", "@types/node"],
    "isolatedModules": false,
    "isolatedDeclarations": false,
    "baseUrl": ".", 
    "paths": {
      "@unative/universal": ["./src"],
      "@unative/universal/*": ["./src/*"] 
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
