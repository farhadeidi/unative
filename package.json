{
  "name": "unative-repo",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "watch": "turbo watch --filter=\"{./packages}...\"",
    "dev:mobile": "turbo dev --filter=\"{./apps/mobile}...\"",
    "dev:web": "turbo dev --filter=\"{./apps/web}...\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "build": "turbo build",
    "build-packages": "turbo build --filter=\"{./packages}...\"",
    "build:mobile": "turbo build --filter=\"...{./apps/mobile}\"",
    "build:web": "turbo build --filter=\"...{./apps/web}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-versions": "pnpm dlx sherif@latest",
    "fix-versions": "pnpm dlx sherif@latest --fix --no-install",
    "clean": "npx ts-node cleaner"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@turbo/gen": "^2.3.3",
    "clean-package": "^2.2.0",
    "cosmiconfig": "^9.0.0",
    "eslint-config-expo": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-project-structure": "^3.13.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "rollup-preserve-directives": "^1.1.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18"
  }
}
